local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Pop It Trading Modded", "GrapeTheme")

local Tab = Window:NewTab("Main")
local Tab2 = Window:NewTab("Player")
local Tab3 = Window:NewTab("Others")
local Tab4 = Window:NewTab("Test items")

local Section = Tab:NewSection("Rare items")
local Section2 = Tab2:NewSection("Player stuff")
local Section3 = Tab3:NewSection("Others")
local sec4 = Tab4:NewSection("Tests")

Section:NewToggle("Buy squid", "", function(state)
    if state then
        if state then _G.on = true while _G.on do wait() local args = { [1] = "Squid" } game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end
    else
        if state then _G.on = true while _G.on do wait() local args = { [1] = "" } game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args)) end else _G.on = false end
    end
end)

